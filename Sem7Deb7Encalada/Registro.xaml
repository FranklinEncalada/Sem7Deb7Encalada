<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Sem7Deb7Encalada.Registro">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Registro"/>
            <Entry x:Name="txtNombre" Placeholder="Ingrese el Nombre" />
            <Entry x:Name="txtUsuario" Placeholder="Ingrese el usuario" />

            <Entry x:Name="txtContrasena" Placeholder="Ingrese la contraseña" IsPassword="True" />
            <Button x:Name="btnRegistro" Text="registro" Clicked="btnRegistro_Clicked"/>
            <Button x:Name="btnRegresar" Text="Regresar" Clicked="btnRegresar_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>